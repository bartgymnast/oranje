{
	"extends": [
		"tslint:recommended"
	],
	"rulesDirectory": [
	],
	"rules": {
		"accessor-pairs": false,
		"array-bracket-newline": false,
		"array-bracket-spacing": false,
		"array-callback-return": false,
		"array-element-newline": false,
		"arrow-body-style": false,
		"arrow-parens": false,
		"arrow-spacing": false,
		"block-scoped-var": false,
		"block-spacing": false,
		"callback-return": false,
		"camelcase": false,
		"capitalized-comments": false,
		"class-methods-use-this": false,
		"comma-style": false,
		"complexity": false,
		"computed-property-spacing": false,
		"consistent-return": false,
		"curly": true,
		"default-case": false,
		"dot-location": false,
		"eol-last": false,
		"eqeqeq": false,
		"func-call-spacing": false,
		"func-name-matching": false,
		"func-names": false,
		"func-style": false,
		"function-paren-newline": false,
		"generator-star-spacing": false,
		"global-require": false,
		"guard-for-in": false,
		"handle-callback-err": false,
		"id-blacklist": false,
		"id-length": false,
		"id-match": false,
		"implicit-arrow-linebreak": false,
		"indent-legacy": false,
		"init-declarations": false,
		"jsx-quotes": false,
		"line-comment-position": false,
		"lines-around-comment": false,
		"lines-around-directive": false,
		"lines-between-class-members": false,
		"max-classes-per-file": false,
		"max-depth": false,
		"max-len": false,
		"max-lines": false,
		"max-lines-per-function": false,
		"max-nested-callbacks": false,
		"max-params": false,
		"max-statements": false,
		"max-statements-per-line": false,
		"multiline-comment-style": false,
		"multiline-ternary": false,
		"new-parens": true,
		"newline-after-var": false,
		"newline-before-return": false,
		"newline-per-chained-call": false,
		"no-alert": false,
		"no-array-constructor": false,
		"no-await-in-loop": false,
		"no-bitwise": false,
		"no-buffer-constructor": false,
		"no-caller": false,
		"no-catch-shadow": false,
		"no-confusing-arrow": false,
		"no-constant-condition": true,
		"no-continue": false,
		"no-control-regex": true,
		"no-debugger": true,
		"no-div-regex": false,
		"no-duplicate-case": true,
		"no-duplicate-imports": false,
		"no-else-return": false,
		"no-empty-character-class": true,
		"no-eq-null": false,
		"no-eval": true,
		"no-ex-assign": true,
		"no-extend-native": false,
		"no-extra-bind": false,
		"no-extra-boolean-cast": true,
		"no-extra-label": false,
		"no-extra-parens": false,
		"no-extra-semi": true,
		"no-floating-decimal": false,
		"no-implicit-coercion": false,
		"no-implicit-globals": false,
		"no-implied-eval": false,
		"no-inline-comments": false,
		"no-inner-declarations": true,
		"no-invalid-regexp": true,
		"no-invalid-this": false,
		"no-irregular-whitespace": true,
		"no-iterator": false,
		"no-label-var": false,
		"no-labels": false,
		"no-lonely-if": false,
		"no-loop-func": false,
		"no-magic-numbers": false,
		"no-mixed-operators": false,
		"no-mixed-requires": false,
		"no-multi-assign": false,
		"no-multi-spaces": false,
		"no-multi-str": false,
		"no-native-reassign": false,
		"no-negated-condition": false,
		"no-negated-in-lhs": false,
		"no-nested-ternary": false,
		"no-new": false,
		"no-new-func": false,
		"no-new-object": false,
		"no-new-require": false,
		"no-new-wrappers": false,
		"no-octal-escape": false,
		"no-param-reassign": false,
		"no-path-concat": false,
		"no-plusplus": false,
		"no-process-env": false,
		"no-process-exit": false,
		"no-proto": false,
		"no-prototype-builtins": false,
		"no-regex-spaces": true,
		"no-restricted-globals": false,
		"no-restricted-imports": false,
		"no-restricted-modules": false,
		"no-restricted-properties": false,
		"no-restricted-syntax": false,
		"no-return-assign": false,
		"no-return-await": false,
		"no-script-url": false,
		"no-self-compare": false,
		"no-sequences": false,
		"no-shadow": false,
		"no-shadow-restricted-names": false,
		"no-spaced-func": false,
		"no-sparse-arrays": true,
		"no-sync": false,
		"no-tabs": false,
		"no-template-curly-in-string": false,
		"no-ternary": false,
		"no-throw-literal": false,
		"no-undef-init": false,
		"no-undefined": false,
		"no-underscore-dangle": false,
		"no-unexpected-multiline": true,
		"no-unmodified-loop-condition": false,
		"no-unneeded-ternary": false,
		"no-unsafe-finally": true,
		"no-unused-expressions": false,
		"no-use-before-define": false,
		"no-useless-call": false,
		"no-useless-computed-key": false,
		"no-useless-concat": false,
		"no-useless-constructor": false,
		"no-useless-rename": false,
		"no-useless-return": false,
		"no-var": false,
		"no-void": false,
		"no-warning-comments": false,
		"no-whitespace-before-property": false,
		"nonblock-statement-body-position": false,
		"object-curly-newline": false,
		"object-property-newline": false,
		"object-shorthand": false,
		"one-var-declaration-per-line": false,
		"operator-assignment": false,
		"operator-linebreak": false,
		"padded-blocks": false,
		"padding-line-between-statements": false,
		"prefer-arrow-callback": false,
		"prefer-const": true,
		"prefer-destructuring": false,
		"prefer-numeric-literals": false,
		"prefer-object-spread": false,
		"prefer-promise-reject-errors": false,
		"prefer-reflect": false,
		"prefer-rest-params": false,
		"prefer-spread": false,
		"prefer-template": false,
		"quotes": false,
		"radix": false,
		"require-await": false,
		"require-jsdoc": false,
		"rest-spread-spacing": false,
		"semi-spacing": false,
		"semi-style": false,
		"sort-imports": false,
		"sort-keys": false,
		"sort-vars": false,
		"space-in-parens": false,
		"space-infix-ops": false,
		"spaced-comment": false,
		"strict": false,
		"switch-colon-spacing": false,
		"symbol-description": false,
		"template-curly-spacing": false,
		"template-tag-spacing": false,
		"unicode-bom": false,
		"use-isnan": true,
		"valid-jsdoc": false,
		"valid-typeof": true,
		"vars-on-top": false,
		"wrap-iife": false,
		"wrap-regex": false,
		"yield-star-spacing": false,
		"yoda": false,

		"brace-style": [ 1, "1tbs", { "allowSingleLine": true } ],
		"consistent-this": [1, "_this", "ctrl", "$ctrl", "that", "self"],
		"comma-dangle": [2, "only-multiline"],
		"space-before-function-paren": [
			true,
			{
				"anonymous": "never",
				"constructor": "never",
				"method": "never",
				"named": "never"
			}
		],
		"quote-props": [2, "as-needed"],
		"space-unary-ops": [2, {"words": true, "nonwords": false}],
		"keyword-spacing": [2, {"after": true}],
		"comma-spacing": [2, {"before": false, "after": true}],
		"key-spacing": [2, {"beforeColon": false, "afterColon": true}],
		"one-var": [1, {
			"uninitialized": "always",
			"initialized": "never"
		}],
		"space-before-blocks": [2, "always"],
		"new-cap": [1, {"capIsNewExceptions": ["TYPES"], "properties": false}],
		"indent": [true, "spaces", 4],
		"linebreak-style": [true, "LF"],
		"semi": [2, "always"],
		"no-console": false,
		"no-empty": false,
		"only-arrow-functions": false,
		"interface-name": false,
		"quotemark": {
			"severity": "warning",
			"options": ["single", "avoid-escape"]
		},
		"trailing-comma": {
			"severity": "warning",
			"options": {
				"multiline": {
					"arrays": "always",
					"objects": "always"
				}
			}
		},
		"align": [true, "elements", "members", "statements"],
		"no-var-requires": false,
		"object-curly-spacing": true,
		"max-line-length": false,
		"array-type": false,
		"member-ordering": {
			"severity": "warning",
			"options": {
				"order": ["instance-field", "constructor", "public-instance-method", "private-instance-method"]
			}
		},
		"ordered-imports": false,
		"one-variable-per-declaration": false,
		"interface-over-type-literal": false,
		"member-access": false,
		"no-consecutive-blank-lines": false,
		"no-inferrable-types": [
			true,
			"ignore-params"
		],
		"no-non-null-assertion": true,
		"no-redundant-jsdoc": true,
		"no-switch-case-fall-through": true,
		"no-use-before-declare": true,
		"object-literal-key-quotes": [
			true,
			"as-needed"
		],
		"object-literal-sort-keys": false,
		"no-output-on-prefix": true,
		"no-inputs-metadata-property": true,
		"no-outputs-metadata-property": true,
		"no-host-metadata-property": true,
		"no-input-rename": true,
		"no-output-rename": true,
		"use-lifecycle-interface": true,
		"use-pipe-transform-interface": true,
		"component-class-suffix": true,
		"eofline": true,
		"no-trailing-whitespace": true,
		"directive-class-suffix": true
	}
}
