{
    "name": "matthillman/schwartz",
    "description": "The Schwartzies website.",
    "keywords": [
        "schwartz"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "babenkoivan/scout-elasticsearch-driver": "^4.0",
        "doctrine/dbal": "^2.7",
        "fabpot/goutte": "^3.2",
        "fideloper/proxy": "^4.0",
        "google/apiclient": "^2.4",
        "graham-campbell/markdown": "^11.1",
        "guzzlehttp/guzzle": "^6.3",
        "laravel-notification-channels/discord": "^1.1",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.1",
        "laravel/horizon": "^3.0.0",
        "laravel/passport": "^8.3",
        "laravel/scout": "^7.2",
        "laravel/socialite": "^4.3",
        "laravel/telescope": "^2.1",
        "laravel/tinker": "^2.1",
        "matthillman/swgoh-help": "dev-master",
        "myclabs/php-enum": "^1.6",
        "nesk/puphpeteer": "^1.6",
        "predis/predis": "^1.1",
        "salsify/json-streaming-parser": "^7.0",
        "spatie/flysystem-dropbox": "^1.0"
    },
    "require-dev": {
        "filp/whoops": "^2.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "resources/helpers/app.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
